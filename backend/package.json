{
  "name": "embedpath-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "tsc",
    "build:vercel": "esbuild api/index.ts --bundle --platform=node --target=node20 --outdir=dist/api --format=esm --external:@supabase/supabase-js --external:hono --external:jose --external:@opennote-ed/sdk",
    "db:generate": "supabase gen types typescript --local > src/types/database.types.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.2",
    "@metaplex-foundation/js": "^0.20.1",
    "@opennote-ed/sdk": "^2.3.3",
    "@solana/web3.js": "^1.98.4",
    "@supabase/supabase-js": "^2.90.1",
    "auth0": "^5.2.0",
    "dotenv": "^16.3.1",
    "hono": "^4.0.0",
    "jose": "^6.1.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "bun-types": "latest",
    "esbuild": "^0.20.0",
    "typescript": "^5.0.0"
  }
}
